{
    "name": "ais",
    "module": "index.ts",
    "type": "module",
    "private": true,
    "bin": {
        "ais": "./dist/ais",
        "aip": "./dist/aip"
    },
    "scripts": {
        "prepare": "effect-language-service patch",
        "build": "bun build './ais.ts' --target node --compile --outfile dist/ais && bun build './aip.ts' --target node --compile --outfile dist/aip",
        "link": "bun link",
        "switch": "bun run build && bun run link"
    },
    "devDependencies": {
        "@effect/language-service": "^0.57.1",
        "@types/bun": "latest"
    },
    "peerDependencies": {
        "typescript": "^5.9.3"
    },
    "dependencies": {
        "@ai-sdk/anthropic": "^2.0.50",
        "@ai-sdk/openai": "^2.0.74",
        "@ai-sdk/openai-compatible": "^1.0.28",
        "@effect/cli": "^0.72.1",
        "@effect/platform-bun": "^0.85.0",
        "@inquirer/prompts": "^8.0.1",
        "ai": "^5.0.104",
        "boxen": "^8.0.1",
        "chalk": "^5.6.2",
        "effect": "^3.19.8",
        "ollama-ai-provider-v2": "^1.5.5"
    }
}
